{
    "manifest_version": 1,
    "name": "How Much Do I Google",
    "description": "Simple extention that logs how many times you Google something every day.",
    "version": "0.0.1",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "tabs"
    ],
    "browser_action": {
    "default_icon": "icon.png",
    "default_title": "How Much Do I Google"
    }
}
